<odoo>
    <!-- View Definitions for Clinic Model -->
    <record id="view_clinic_admin_form" model="ir.ui.view">
        <field name="name">clinic.admin.form</field>
        <field name="model">clinic.clinic</field>
        <field name="arch" type="xml">
            <form string="Clinic">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="address"/>
                        <field name="contact_info"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_clinic_admin_list" model="ir.ui.view">
        <field name="name">clinic.admin.list</field>
        <field name="model">clinic.clinic</field>
        <field name="arch" type="xml">
            <list string="Clinics">
                <field name="name"/>
                <field name="address"/>
                <field name="contact_info"/>
            </list>
        </field>
    </record>

    <!-- View Definitions for Medical Specialist Model -->
    <record id="view_medical_specialist_admin_form" model="ir.ui.view">
        <field name="name">medical.specialist.admin.form</field>
        <field name="model">clinic.medical_specialist</field>
        <field name="arch" type="xml">
            <form string="Medical Specialist">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="specialization"/>
                        <field name="contact_info"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_medical_specialist_admin_list" model="ir.ui.view">
        <field name="name">medical.specialist.admin.list</field>
        <field name="model">clinic.medical_specialist</field>
        <field name="arch" type="xml">
            <list string="Medical Specialists">
                <field name="name"/>
                <field name="specialization"/>
                <field name="contact_info"/>
            </list>
        </field>
    </record>

    <!-- View Definitions for Patient Model -->
    <record id="view_patient_admin_form" model="ir.ui.view">
        <field name="name">patient.admin.form</field>
        <field name="model">clinic.patient</field>
        <field name="arch" type="xml">
            <form string="Patient">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="birth_date"/> <!-- Corrected field name -->
                        <field name="medical_history"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_patient_admin_list" model="ir.ui.view">
        <field name="name">patient.admin.list</field>
        <field name="model">clinic.patient</field>
        <field name="arch" type="xml">
            <list string="Patients">
                <field name="name"/>
                <field name="birth_date"/> <!-- Corrected field name -->
                <field name="medical_history"/>
            </list>
        </field>
    </record>

    <!-- View Definitions for Appointment Model -->
    <record id="view_appointment_admin_form" model="ir.ui.view">
        <field name="name">appointment.admin.form</field>
        <field name="model">clinic.appointment</field>
        <field name="arch" type="xml">
            <form string="Appointment">
                <sheet>
                    <group>
                        <field name="patient_id"/>
                        <field name="doctor_id"/>
                        <field name="date"/>
                        <field name="notes"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_appointment_admin_list" model="ir.ui.view">
        <field name="name">appointment.admin.list</field>
        <field name="model">clinic.appointment</field>
        <field name="arch" type="xml">
            <list string="Appointments">
                <field name="patient_id"/>
                <field name="doctor_id"/>
                <field name="date"/>
                <field name="notes"/>
            </list>
        </field>
    </record>

    <!-- View Definitions for Announcement Model -->
    <record id="view_announcement_admin_form" model="ir.ui.view">
        <field name="name">announcement.admin.form</field>
        <field name="model">clinic.announcement</field>
        <field name="arch" type="xml">
            <form string="Announcement">
                <sheet>
                    <group>
                        <field name="title"/>
                        <field name="content"/>
                        <field name="created_date"/>
                        <field name="expiry_date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_announcement_admin_list" model="ir.ui.view">
        <field name="name">announcement.admin.list</field>
        <field name="model">clinic.announcement</field>
        <field name="arch" type="xml">
            <list string="Announcements">
                <field name="title"/>
                <field name="content"/>
                <field name="created_date"/>
                <field name="expiry_date"/>
            </list>
        </field>
    </record>

    <!-- Action Definitions -->
    <record id="action_clinic_admin_list" model="ir.actions.act_window">
        <field name="name">Clinics</field>
        <field name="res_model">clinic.clinic</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_clinic_admin_list"/>
        <field name="view_id" ref="view_clinic_patient_list"/>
        <field name="view_ids" eval="[(0, 0, {'view_id': ref('view_clinic_admin_list'), 'view_mode': 'list'}), (0, 0, {'view_id': ref('view_clinic_admin_form'), 'view_mode': 'form'})]"/>
    </record>

    <record id="action_medical_specialist_admin_list" model="ir.actions.act_window">
        <field name="name">Medical Specialists</field>
        <field name="res_model">clinic.medical_specialist</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_medical_specialist_admin_list"/>
        <field name="view_ids" eval="[(0, 0, {'view_id': ref('view_medical_specialist_admin_list'), 'view_mode': 'list'}), (0, 0, {'view_id': ref('view_medical_specialist_admin_form'), 'view_mode': 'form'})]"/>
    </record>

    <record id="action_patient_admin_list" model="ir.actions.act_window">
        <field name="name">Patients</field>
        <field name="res_model">clinic.patient</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_patient_admin_list"/>
        <field name="view_ids" eval="[(0, 0, {'view_id': ref('view_patient_admin_list'), 'view_mode': 'list'}), (0, 0, {'view_id': ref('view_patient_admin_form'), 'view_mode': 'form'})]"/>
    </record>

    <record id="action_appointment_admin_list" model="ir.actions.act_window">
        <field name="name">Appointments</field>
        <field name="res_model">clinic.appointment</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_appointment_admin_list"/>
        <field name="view_ids" eval="[(0, 0, {'view_id': ref('view_appointment_admin_list'), 'view_mode': 'list'}), (0, 0, {'view_id': ref('view_appointment_admin_form'), 'view_mode': 'form'})]"/>
    </record>

    <record id="action_announcement_admin_list" model="ir.actions.act_window">
        <field name="name">Announcements</field>
        <field name="res_model">clinic.announcement</field>
        <field name="view_mode">list,form</field>
        <field name="view_id" ref="view_announcement_admin_list"/>
        <field name="view_ids" eval="[(0, 0, {'view_id': ref('view_announcement_admin_list'), 'view_mode': 'list'}), (0, 0, {'view_id': ref('view_announcement_admin_form'), 'view_mode': 'form'})]"/>
    </record>

    <!-- Menu Definitions -->
    <menuitem id="menu_techcare_root" name="TechCare" sequence="1"/>
    <menuitem id="menu_clinic" name="Clinics" parent="menu_techcare_root" action="action_clinic_admin_list" sequence="10"/>
    <menuitem id="menu_medical_specialist" name="Medical Specialists" parent="menu_techcare_root" action="action_medical_specialist_admin_list" sequence="20"/>
    <menuitem id="menu_patient" name="Patients" parent="menu_techcare_root" action="action_patient_admin_list" sequence="30"/>
    <menuitem id="menu_appointment" name="Appointments" parent="menu_techcare_root" action="action_appointment_admin_list" sequence="40"/>
    <menuitem id="menu_announcement" name="Announcements" parent="menu_techcare_root" action="action_announcement_admin_list" sequence="50"/>
</odoo>
